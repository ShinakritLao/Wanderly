{
  "expo": {
    "name": "wanderly",
    "slug": "wanderly",
    "scheme": "wanderly",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon1.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/splash-icon1.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "ios": {
      "bundleIdentifier": "com.wanderly.app",
      "supportsTablet": true
    },
    "android": {
      "package": "com.wanderly.app",
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon1.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./src/assets/favicon1.png"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "kotlinVersion": "2.0.0"
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "e7a21459-8eae-4cf6-8020-fb68bd2b2345"
      }
    }
  }
}