{
  "expo": {
    "name": "wanderly",
    "slug": "wanderly",
    "scheme": "wanderly",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon1.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./src/assets/splash-icon1.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "platforms": ["ios", "android", "web"],
    "ios": {
      "bundleIdentifier": "com.wanderly.app",
      "supportsTablet": true
    },
    "android": {
      "package": "com.wanderly.app",
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon1.png",
        "backgroundColor": "#ffffff"
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "wanderly",
              "host": "*"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "favicon": "./src/assets/favicon1.png",
      "bundler": "webpack",
      "output": "static"
    }
  }
}
