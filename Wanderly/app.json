{
  "expo": {
    "name": "Wanderly",
    "slug": "wanderly",
    "scheme": "wanderly",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon1.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon1.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff",
    "platforms": ["ios", "android", "web"]
    },
    "ios": {
      "bundleIdentifier": "com.wanderly.app",
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon1.png",
        "package": "com.wanderly.app",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon1.png"
    }
  }
}
